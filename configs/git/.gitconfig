[ghq]
	root = ~/dev/src
[user]
	name = yayoc
	email = n.yayoshi+github@gmail.com
[credential]
	helper = osxkeychain
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[core]
    editor = vim
[color]
	ui = true
	diff = true
[alias]
	s = status
	d = diff
	co = checkout
	br = branch
	last = log -1 HEAD
	cane = commit --amend --no-edit
	lo = log --oneline -n 10
	pr = pull --rebase
